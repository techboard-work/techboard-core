<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Event.
    -->
    <changeSet id="20230405112523-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="environment_id"
                                 baseTableName="event"
                                 constraintName="fk_event__environment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="environment"/>

        <addForeignKeyConstraint baseColumnNames="reporter_id"
                                 baseTableName="event"
                                 constraintName="fk_event__reporter_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>
    </changeSet>
</databaseChangeLog>
